{% extends 'base.html.twig' %}

{% block title %}
    Form
{% endblock %}

{% block body %}
  <div class="wrapper">
    {{ form_start(form) }}
         <div>
            {{ form_label(form.name) }}
            {{ form_widget(form.name) }}
            <span>
                {{ form_errors(form.name) }}
            </span>
         </div>
    {{ form_end(form, {'render_rest': false}) }}    
  </div>
{% endblock %}
